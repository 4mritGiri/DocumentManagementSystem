{
  "version": 2,
  "builds": [
      {
          "src": "DocumentManagementSystem/wsgi.py",
          "use": "@vercel/python",
          "config": { "maxLambdaSize": "15mb", "runtime": "python3.11" }
      },
      {
          "src": "build_files.sh",
          "use": "@vercel/static-build",
          "config": {
              "distDir": "staticfiles"
          }
      }
  ],
  "rewrites": [
      {
          "source": "/(.*)",
          "destination": "DocumentManagementSystem/wsgi.py"
      }
  ]
}
